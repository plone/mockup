    "resolutions": {
        "backbone": "1.1.2",
        "underscore": "1.12.0",
        "jqtree": "1.4.10",
        "jquery": "3.5.1",
        "datatables.net": "1.10.23",
        "datatables.net-bs": "1.10.23"
    },
    "scripts": {
        "clean": "rimraf docs/_site",
        "build:webpack": "webpack --config webpack.config.js --info-verbosity verbose --env.NODE_ENV=production",
        "build:docs": "ELEVENTY_ENV=production  npx @11ty/eleventy --output docs/_site/",
        "build": "run-s clean build:webpack build:docs",
        "watch:webpack": "webpack --config webpack.config.js --watch --env.NODE_ENV=development",
        "build-stats": "webpack --config webpack.config.js --env.NODE_ENV=production --json > stats.json",
        "test": "jest --watch",
        "testonce": "jest",
        "postinstall": "patch --forward node_modules/select2/select2.js < patches/select2.patch || true",
        "collect:externaldocs": "copyup -V node_modules/patternslib/src/pat/**/documentation.md node_modules/pat-*/README.md docs/external/",
        "start:webpack": "webpack-dev-server --config webpack.config.js --info-verbosity verbose --env.NODE_ENV=development",
        "start:docs": "ELEVENTY_ENV=development npx @11ty/eleventy --serve --output docs/_site/",
        "start": "npm-run-all clean --parallel start:*"
    },
    "browserslist": [
        ">0.2%",
        "ie >= 11",
        "not dead"
    ],
    "maintainers": [
        {
            "name": "Plone Foundation",
            "email": "plone-developers@lists.sourceforge.net",
            "url": "http://plone.org"
        }
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/plone/mockup.git"
    },
    "bugs": {
        "url": "https://github.com/plone/mockup/issues",
