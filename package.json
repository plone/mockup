    "scripts": {
        "clean": "rimraf docs/_site",
        "build:webpack": "webpack --config webpack.config.js --info-verbosity verbose --mode=production",
        "build:webpack:plone": "webpack --config webpack.config.js --info-verbosity verbose --mode=production --env.DEPLOYMENT=plone",
        "build:docs": "ELEVENTY_ENV=production  npx @11ty/eleventy --output docs/_site/",
        "build:stats": "webpack --config webpack.config.js --mode=production --json > stats.json",
        "build": "run-s clean build:webpack build:docs",
        "watch:webpack": "webpack --config webpack.config.js --watch --mode=development",
        "watch:webpack:plone": "webpack --config webpack.config.js --watch --mode=production --env.DEPLOYMENT=plone",
        "postinstall": "patch --forward node_modules/select2/select2.js < patches/select2.patch || true",
        "collect:externaldocs": "copyup -V node_modules/patternslib/src/pat/**/documentation.md node_modules/pat-*/README.md docs/external/",
        "start:webpack": "webpack-dev-server --config webpack.config.js --info-verbosity verbose --mode=development",
        "start:docs": "ELEVENTY_ENV=development npx @11ty/eleventy --serve --port 8001 --output docs/_site/",
        "start": "npm-run-all clean --parallel start:*",
        "test": "jest",
        "testwatch": "jest --watch"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ]
    },
    "publishConfig": {
        "access": "public"
